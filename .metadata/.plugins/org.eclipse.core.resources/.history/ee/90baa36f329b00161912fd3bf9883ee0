package extrafront;

public class ExtraFrontRunner {
	public static void main(String[] args) {
		ExtraFront test = new ExtraFront();
		String var;
		
		var = test.extraFront("Hello");
		System.out.println(var);
		
		var = test.extraFront("o");
		System.out.println(var);
		
		//----------------------------------
		
		var = test.extraBack("Hello");
		System.out.println(var);
	}
}
