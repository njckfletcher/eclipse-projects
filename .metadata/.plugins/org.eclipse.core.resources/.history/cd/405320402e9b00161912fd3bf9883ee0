package seecolor;

public class SeeColor {
	public String seeColor(String str) {
		String text = str;
		
		if(text.length() > 2) {
			String firstThree = text.substring(0, 3);
			String firstFour = text.substring(0, 4);
			
			if(firstThree.contains("red")) {
				text = "red";
			}
			if(firstFour.contains("blue")) {
				text = "blue";
			}
		}
		return text;
	}

}
