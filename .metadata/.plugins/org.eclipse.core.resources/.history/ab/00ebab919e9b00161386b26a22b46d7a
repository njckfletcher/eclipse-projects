package greenticket;

public class GreenTicket {
	public int greenTicket(int a, int b, int c) {
		if(a != b && b != c && a != c) {
			return 0;
		}
		else if(a == b && a == c && b == c) {
			return 20;
		}
		else if((a == b && b == c) || (a == b && a == c) || (b == c && a == c)) {
			return 10;
		}
		else {
			return 30;
		}
	}

}
