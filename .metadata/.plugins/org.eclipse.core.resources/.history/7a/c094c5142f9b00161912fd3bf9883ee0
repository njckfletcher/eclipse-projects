package seecolor;

public class SeeColor {
	public String seeColor(String str) {
		String text = str;
		
		if(text.length() > 2) {
			String firstThree;
			String firstFour;
			
			if(text.length() >= 3) {
				firstThree = text.substring(0, 3);
				firstFour = text.substring(0, 4);
				
				if(firstThree.contains("red")) {
					text = "red";
				}
				if(firstFour.contains("blue")) {
					text = "blue";
				}
			}
			else {
				
			}
		}
		else {
			text = "";
		}
		return text;
	}

}
