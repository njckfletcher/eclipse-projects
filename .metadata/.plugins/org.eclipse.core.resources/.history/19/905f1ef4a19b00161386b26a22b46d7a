package inorder;

public class InOrder {
	public boolean inOrder(int a, int b, int c, boolean bOk) {
		boolean finalValue;
		
		if(bOk == false && b > a && c > b) {
			return true;
		}
		else {
			return false;
		}
		
		if(bOk == true && c > a) {
			return true;
		}
		else {
			return false;
		}
	}
}
